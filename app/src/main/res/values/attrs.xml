<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuItemProfileView">
        <attr name="iconSrc" format="reference" />
        <attr name="labelText" format="string" />
        <attr name="buttonHidden" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HeaderView">
        <attr name="title" format="string" />
        <attr name="headerButtonHidden" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BottomMenuView">
        <attr name="page" format="string">
            <enum name="home" value="0" />
            <enum name="profile" value="1" />
            <enum name="calendar" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="LargeCustomButtonView">
        <attr name="labelTextButton" format="string" />
        <attr name="active" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CalendarItemView">
        <attr name="day_number" format="string" />
        <attr name="day_name" format="string" />
        <attr name="type" format="string">
            <enum name="active" value="0" />
            <enum name="inactive" value="1" />
            <enum name="today" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="HourItemView">
        <attr name="hour" format="string" />
        <attr name="expedient" format="string">
            <enum name="am" value="0" />
            <enum name="pm" value="1" />
        </attr>
        <attr name="typeTime" format="string">
            <enum name="inactive" value="0" />
            <enum name="active" value="1" />
            <enum name="selected" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ButtonTagView">
        <attr name="titleTag" format="string" />
        <attr name="typeTag" format="string">
            <enum name="inactive" value="0" />
            <enum name="active" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="InputView">
        <attr name="titleInput" format="string" />
        <attr name="placeholder" format="string" />
        <attr name="typeInput" format="string">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="email" value="2" />
            <enum name="phone" value="3" />
            <enum name="large" value="4" />
        </attr>
    </declare-styleable>
</resources>